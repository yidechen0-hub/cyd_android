<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:largeHeap="true"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cyd_android">
        <activity
            android:name=".activity.MainActivity"
            android:exported="true"/>

        <activity
            android:name=".activity.BroadcastActivity"
            android:exported="true" />

        <receiver android:name=".receiver.MyStaticReceiver" />
        <provider
            android:name=".contentProvider.Students"
            android:authorities="com.cyd.demo"
            android:exported="true" />
        <activity
            android:name=".activity.AdapterActivity"
            android:exported="true" />

        <activity
            android:name=".activity.trailPluginActivity"
            android:exported="true" />

        <activity
            android:name=".activity.SerializationActivity"
            android:exported="true" />


        <activity android:name=".activity.GetSysLanguageActivity"
            android:exported="true"/>

        <activity android:name=".activity.RecyclerViewActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>




        <service android:name=".serialization.TestService"
            android:exported="true" />


    </application>

</manifest>